<h1>List of all directors</h1>

<table border="1">
<% @list_of_directors.all.each do |a_thing| %>
<tr>
  <td><%= a_thing.id %></td>
  <td>
    <img src="<%= a_thing.image %>">
  </td>
  <td><%= a_thing.name %></td>
  <td><%= a_thing.dob %></td>
  <td>
    <a href="/director/<%= a_thing.id %>">
    Showdetails
    </a>
  </td>
</tr>
<% end %>
</table>
